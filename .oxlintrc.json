{
  "plugins": ["react", "typescript"],
  "rules": {
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-empty-function": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": ["warn", {
      "varsIgnorePattern": "_",
      "argsIgnorePattern": "_"
    }],
    "prefer-const": "off"
  }
}
